def read_file(file_name):
    f=open(file_name,'r')
    n = int(f.readline())
    bonus_points = []
    for i in range(n):
        x, y, reward = f.readline().strip().split(' ')
        bonus_points.append((x, y, reward))
    matrix = []
    for line in f.read().splitlines():
        matrix.append(list(line))
    f.close()

    return bonus_points, matrix
